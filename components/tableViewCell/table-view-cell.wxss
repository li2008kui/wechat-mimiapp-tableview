.table_view_cell {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-left: 24rpx;
  padding-right: 24rpx;
  border-top: 1px solid #e0e0e0;
}

.cell_selected {
  background-color: #dddddd;
}

.cell_un_selected {
  background-color: white;
}

.content_view {
  display: flex;
  align-items: center;
}

.content_view>view {
  display: flex;
  flex-direction: column;
}

.main_image {
  width: 48rpx;
  height: 48rpx;
  margin-right: 20rpx;
}

.text_label {
  font-size: 15px;
}

.detail_text_label {
  display: none;
  font-size: 13px;
  color: #999999;
}

.indicate_icon {
  width: 32rpx;
  height: 32rpx;
}

.subtitle .detail_text_label {
  display: block;
}

.value1>.content_view>view {
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.value1 .detail_text_label {
  display: block;
  font-size: 14px;
  color: #3d588a;
}

.value2 .main_image {
  display: none;
}

.value2>.content_view>view {
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.value2 .text_label {
  font-size: 12px;
  color: #3d588a;
  margin-right: 15rpx;
}

.value2 .detail_text_label {
  display: block;
  font-size: 14px;
  color: black;
}